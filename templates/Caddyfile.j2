{{ ansible_managed | comment }}

{
    debug
    admin off
    log {
        output file /var/log/caddy/access.log {
                roll_size 1gb
                roll_keep 5
                roll_keep_for 720h
        }
    }
}

# There is an implicit rule like the one below at the end of the file.
https:// {
  respond 200
}



